{
  "extends": "@qra-website/tsconfig/next.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "paths": {
      "@/*": ["./src/*"],
      "@public/*": ["./public/*"]
    }
  },
  "include": [
    "public/**/*",
    "next-env.d.ts",
    "next.config.js",
    "postcss.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".storybook/**/*",
    "sst-env.d.ts"
  ],
  "exclude": ["node_modules"]
}
